{
  "name": "interactiveimagejs",
  "version": "2.7.1",
  "description": "A jQuery plugin to embed interactive images on your website",
  "author": {
    "name": "Jean-Philippe Chateau",
    "email": "contact@jpchateau.com",
    "url": "https://www.jpchateau.com/"
  },
  "browser": "./src/js/index.js",
  "license": "MIT",
  "scripts": {
    "build": "webpack --progress --colors --config webpack.prod.js",
    "dev": "webpack --progress --colors --config webpack.dev.js",
    "eslint": "./node_modules/.bin/eslint ./src/js/",
    "sass-lint": "./node_modules/.bin/sass-lint --verbose --no-exit",
    "start:dev": "./node_modules/.bin/webpack-dev-server --config webpack.dev.js",
    "test": "./node_modules/.bin/mocha",
    "test-with-coverage": "./node_modules/.bin/cross-env NODE_ENV=test ./node_modules/.bin/nyc ./node_modules/.bin/_mocha",
    "watch": "webpack --watch --colors --config webpack.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpchateau/Interactive-Image.git"
  },
  "keywords": [
    "interactive",
    "image",
    "picture",
    "photo",
    "audio",
    "sound",
    "video",
    "tooltip",
    "image note",
    "image marker",
    "image effects",
    "storytelling",
    "media experience",
    "interactive popover",
    "interactive presentation",
    "media links",
    "multimedia content",
    "rich content",
    "javascript",
    "jquery-plugin",
    "ecosystem:jquery",
    "es2020"
  ],
  "homepage": "https://github.com/jpchateau/Interactive-Image#readme",
  "bugs": {
    "url": "https://github.com/jpchateau/Interactive-Image/issues"
  },
  "dependencies": {
    "imagesloaded": "^4.1.4"
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.10.5",
    "@babel/preset-env": "7.10.4",
    "@babel/register": "7.10.5",
    "babel-loader": "8.1.0",
    "babel-plugin-add-module-exports": "1.0.2",
    "babel-plugin-istanbul": "6.0.0",
    "chai": "4.2.0",
    "cross-env": "7.0.2",
    "css-loader": "3.6.0",
    "eslint": "7.5.0",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.3.0",
    "jquery": "3.5.1",
    "jsdom": "16.3.0",
    "jsdom-global": "3.0.2",
    "mini-css-extract-plugin": "0.9.0",
    "mocha": "8.0.1",
    "node-sass": "7.0.0",
    "nyc": "15.1.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "sass-lint": "1.13.1",
    "sass-loader": "9.0.2",
    "style-loader": "1.2.1",
    "uglifyjs-webpack-plugin": "2.2.0",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "5.0.9"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
